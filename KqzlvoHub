local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

-- Clear old UI
for _, gui in pairs(PlayerGui:GetChildren()) do
    if gui.Name == "KqzlvoHub" then gui:Destroy() end
end

-- Settings
local Settings = {
    AutoTime = false, PerfectTime = 0.7, EarlyTime = 0.1,
    AutoSprint = false, InfiniteStamina = false,
    SpeedBoost = false, OnBallSpeed = 25, OffBallSpeed = 20,
    DisableMovementLock = false, DribbleMacro = false,
    AntiOOB = true, DribbleSpeedBoost = false,
    AutoGuard = false, AutoBlock = false,
    DisableTurnLock = false, AutoSteal = false,
    SpinBot = false, SprintGuard = false
}

