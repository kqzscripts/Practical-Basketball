ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y + 20)

-- MAIN FEATURES LOOP
spawn(function()
    while wait(0.1) do
        local Character = Player.Character
        if Character and Character:FindFirstChild("Humanoid") and Character:FindFirstChild("HumanoidRootPart") then
            local Humanoid = Character.Humanoid
            local RootPart = Character.HumanoidRootPart
            
            -- SPEED BOOST
            if Settings.SpeedBoost then
                Humanoid.WalkSpeed = (RootPart.Velocity.Magnitude > 25 and Settings.OnBallSpeed or Settings.OffBallSpeed)
            end
            
            -- AUTO SPRINT
            if Settings.AutoSprint then
                Humanoid:ChangeState(Enum.HumanoidStateType.Running)
            end
            
            -- ANTI OOB
            if Settings.AntiOOB and RootPart.Position.Y < -45 then
                RootPart.CFrame = CFrame.new(RootPart.Position.X, 10, RootPart.Position.Z)
            end
        end
    end
end)

