-- KqzlvoHub v1.0 | Practical Basketball
-- Load: Copy entire script → Paste in executor → Execute

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")

local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

-- Anti-Detection
getgenv().KqzlvoHub = true

-- Variables
local Settings = {
    AutoTime = {Enabled = false, PerfectTime = 0.7, EarlyTime = 0.1},
    AutoSprint = false,
    InfiniteStamina = false,
    SpeedBoost = {Enabled = false, OnBall = 25, OffBall = 20},
    DisableMovementLock = false,
    DribbleMacro = false,
    AntiOOB = true,
    DribbleSpeedBoost = false,
    AutoGuard = false,
    AutoBlock = false,
    DisableTurnLock = false,
    AutoSteal = false,
    SpinBot = false,
    SprintGuard = false
}

-- Create Fancy Luxury UI
local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local CloseBtn = Instance.new("TextButton")
local Tabs = Instance.new("Frame")
local TabContent = Instance.new("Frame")

-- Luxury Design Setup
local function CreateUI()
    ScreenGui.Name = "KqzlvoHub"
    ScreenGui.Parent = PlayerGui
    ScreenGui.ResetOnSpawn = false
    
    -- Main Frame (Luxury Glass Effect)
    MainFrame.Name = "MainFrame"
    MainFrame.Parent = ScreenGui
    MainFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 25)
    MainFrame.BorderSizePixel = 0
    MainFrame.Position = UDim2.new(0.5, -250, 0.5, -200)
    MainFrame.Size = UDim2.new(0, 500, 0, 400)
    MainFrame.Active = true
    MainFrame.Draggable = true
    
    -- Luxury Corner Rounding
    local MainCorner = Instance.new("UICorner")
    MainCorner.CornerRadius = UDim.new(0, 16)
    MainCorner.Parent = MainFrame
    
    local MainStroke = Instance.new("UIStroke")
    MainStroke.Color = Color3.fromRGB(100, 200, 255)
    MainStroke.Thickness = 2
    MainStroke.Parent = MainFrame
    
    -- Gradient Background
    local Gradient = Instance.new("UIGradient")
    Gradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(20, 20, 40)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(10, 10, 25))
    }
    Gradient.Rotation = 45
    Gradient.Parent = MainFrame
    
    -- Title Bar
    Title.Name = "Title"
    Title.Parent = MainFrame
    Title.BackgroundTransparency = 1
    Title.Position = UDim2.new(0, 20, 0, 15)
    Title.Size = UDim2.new(1, -60, 0, 35)
    Title.Font = Enum.Font.GothamBold
    Title.Text = "⭐ KqzlvoHub v1.0 ⭐"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextScaled = true
    Title.TextXAlignment = Enum.TextXAlignment.Left
    
    -- Close Button
    CloseBtn.Name = "CloseBtn"
    CloseBtn.Parent = MainFrame
    CloseBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    CloseBtn.BorderSizePixel = 0
    CloseBtn.Position = UDim2.new(1, -45, 0, 12)
    CloseBtn.Size = UDim2.new(0, 30, 0, 30)
    CloseBtn.Font = Enum.Font.GothamBold
    CloseBtn.Text = "✕"
    CloseBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    CloseBtn.TextScaled = true
    
    local CloseCorner = Instance.new("UICorner")
    CloseCorner.CornerRadius = UDim.new(0, 8)
    CloseCorner.Parent = CloseBtn
    
    -- Tabs Frame
    Tabs.Name = "Tabs"
    Tabs.Parent = MainFrame
    Tabs.BackgroundTransparency = 1
    Tabs.Position = UDim2.new(0, 0, 0, 60)
    Tabs.Size = UDim2.new(0, 120, 1, -60)
    
    -- Tab Content
    TabContent.Name = "TabContent"
    TabContent.Parent = MainFrame
    TabContent.BackgroundTransparency = 1
    TabContent.Position = UDim2.new(0, 120, 0, 60)
    TabContent.Size = UDim2.new(1, -120, 1, -60)
end

-- Create Tabs
local function CreateTab(name, pos)
    local TabBtn = Instance.new("TextButton")
    TabBtn.Name = name .. "Tab"
    TabBtn.Parent = Tabs
    TabBtn.BackgroundColor3 = Color3.fromRGB(35, 35, 55)
    TabBtn.BorderSizePixel = 0
    TabBtn.Position = UDim2.new(0, 0, 0, pos)
    TabBtn.Size = UDim2.new(1, 0, 0, 45)
    TabBtn.Font = Enum.Font.GothamSemibold
    TabBtn.Text = name
    TabBtn.TextColor3 = Color3.fromRGB(200, 200, 255)
    TabBtn.TextScaled = true
    
    local TabCorner = Instance.new("UICorner")
    TabCorner.CornerRadius = UDim.new(0, 10)
    TabCorner.Parent = TabBtn
    
    return TabBtn
end

-- Create Toggle
local function CreateToggle(parent, name, callback)
    local ToggleFrame = Instance.new("Frame")
    ToggleFrame.Name = name
    ToggleFrame.Parent = parent
    ToggleFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 40)
    ToggleFrame.BorderSizePixel = 0
    ToggleFrame.Size = UDim2.new(1, -20, 0, 40)
    
    local Corner = Instance.new("UICorner")
    Corner.CornerRadius = UDim.new(0, 10)
    Corner.Parent = ToggleFrame
    
    local Label = Instance.new("TextLabel")
    Label.Parent = ToggleFrame
    Label.BackgroundTransparency = 1
    Label.Position = UDim2.new(0, 10, 0, 0)
    Label.Size = UDim2.new(0.7, 0, 1, 0)
    Label.Font = Enum.Font.Gotham
    Label.Text = name
    Label.TextColor3 = Color3.fromRGB(255, 255, 255)
    Label.TextXAlignment = Enum.TextXAlignment.Left
    Label.TextScaled = true
    
    local ToggleBtn = Instance.new("TextButton")
    ToggleBtn.Parent = ToggleFrame
    ToggleBtn.BackgroundColor3 = Color3.fromRGB(100, 200, 100)
    ToggleBtn.BorderSizePixel = 0
    ToggleBtn.Position = UDim2.new(1, -50, 0, 5)
    ToggleBtn.Size = UDim2.new(0, 40, 0, 30)
    ToggleBtn.Font = Enum.Font.GothamBold
    ToggleBtn.Text = "ON"
    ToggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleBtn.TextScaled = true
    
    local BtnCorner = Instance.new("UICorner")
    BtnCorner.CornerRadius = UDim.new(0, 8)
    BtnCorner.Parent = ToggleBtn
    
    local toggled = false
    ToggleBtn.MouseButton1Click:Connect(function()
        toggled = not toggled
        Settings[name:gsub(" ", "")] = toggled
        TweenService:Create(ToggleBtn, TweenInfo.new(0.2), {
            BackgroundColor3 = toggled and Color3.fromRGB(100, 200, 100) or Color3.fromRGB(200, 100, 100)
        }):Play()
        ToggleBtn.Text = toggled and "ON" or "OFF"
        callback(toggled)
    end)
end

-- Create Slider
local function CreateSlider(parent, name, min, max, default, callback)
    local SliderFrame = Instance.new("Frame")
    SliderFrame.Name = name
    SliderFrame.Parent = parent
    SliderFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 40)
    SliderFrame.BorderSizePixel = 0
    SliderFrame.Size = UDim2.new(1, -20, 0, 60)
    
    local Corner = Instance.new("UICorner")
    Corner.CornerRadius = UDim.new(0, 10)
    Corner.Parent = SliderFrame
    
    local Label = Instance.new("TextLabel")
    Label.Parent = SliderFrame
    Label.BackgroundTransparency = 1
    Label.Position = UDim2.new(0, 10, 0, 5)
    Label.Size = UDim2.new(0.6, 0, 0.4, 0)
    Label.Font = Enum.Font.Gotham
    Label.Text = name .. ": " .. default
    Label.TextColor3 = Color3.fromRGB(255, 255, 255)
    Label.TextXAlignment = Enum.TextXAlignment.Left
    Label.TextScaled = true
    
    local SliderBar = Instance.new("Frame")
    SliderBar.Parent = SliderFrame
    SliderBar.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
    SliderBar.BorderSizePixel = 0
    SliderBar.Position = UDim2.new(0, 10, 0.6, 0)
    SliderBar.Size = UDim2.new(0.8, 0, 0, 8)
    
    local SliderCorner = Instance.new("UICorner")
    SliderCorner.CornerRadius = UDim.new(0, 4)
    SliderCorner.Parent = SliderBar
    
    local SliderFill = Instance.new("Frame")
    SliderFill.Parent = SliderBar
    SliderFill.BackgroundColor3 = Color3.fromRGB(100, 200, 255)
    SliderFill.BorderSizePixel = 0
    SliderFill.Size = UDim2.new((default-min)/(max-min), 0, 1, 0)
    
    local FillCorner = Instance.new("UICorner")
    FillCorner.CornerRadius = UDim.new(0, 4)
    FillCorner.Parent = SliderFill
    
    local dragging = false
    SliderFill.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
        end
    end)
    
    SliderFill.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local mouse = Player:GetMouse()
            local percent = math.clamp((mouse.X - SliderBar.AbsolutePosition.X) / SliderBar.AbsoluteSize.X, 0, 1)
            SliderFill.Size = UDim2.new(percent, 0, 1, 0)
            local value = math.floor(min + (max - min) * percent)
            Label.Text = name .. ": " .. value
            callback(value)
        end
    end)
end

-- Initialize UI
CreateUI()

-- Create Tabs
local MovementTab = CreateTab("Movement", 0)
local CombatTab = CreateTab("Combat", 45)
local DribbleTab = CreateTab("Dribble", 90)
local MiscTab = CreateTab("Misc", 135)

-- Tab Content Frames
local MovementContent = Instance.new("ScrollingFrame")
MovementContent.Parent = TabContent
MovementContent.BackgroundTransparency = 1
MovementContent.Position = UDim2.new(0, 0, 0, 0)
MovementContent.Size = UDim2.new(1, 0, 1, 0)
MovementContent.ScrollBarThickness = 6
MovementContent.CanvasSize = UDim2.new(0, 0, 0, 400)

local CombatContent = Instance.new("ScrollingFrame")
CombatContent.Parent = TabContent
CombatContent.BackgroundTransparency = 1
CombatContent.Position = UDim2.new(1, -400, 0, 0)
CombatContent.Size = UDim2.new(1, 0, 1, 0)
CombatContent.ScrollBarThickness = 6
CombatContent.CanvasSize = UDim2.new(0, 0, 0, 400)

-- Close Button
CloseBtn.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

-- Features Implementation
local Character = Player.CharacterAdded:Wait():WaitForChild("HumanoidRootPart")
local Humanoid = Character:WaitForChild("Humanoid")

-- Auto Time with Sliders
CreateSlider(MovementContent, "Perfect Time", 0.1, 1.5, 0.7, function(value)
    Settings.AutoTime.PerfectTime = value
end)
CreateSlider(MovementContent, "Early Time", 0.01, 0.5, 0.1, function(value)
    Settings.AutoTime.EarlyTime = value
end)
CreateToggle(MovementContent, "Auto Time", function(state)
    Settings.AutoTime.Enabled = state
end)

-- Speed Features
CreateToggle(MovementContent, "Auto Sprint", function(state)
    Settings.AutoSprint = state
end)

CreateToggle(MovementContent, "Infinite Stamina", function(state)
    Settings.InfiniteStamina = state
end)

CreateSlider(MovementContent, "On Ball Speed", 16, 50, 25, function(value)
    Settings.SpeedBoost.OnBall = value
end)
CreateSlider(MovementContent, "Off Ball Speed", 16, 50, 20, function(value)
    Settings.SpeedBoost.OffBall = value
end)

CreateToggle(MovementContent, "Speed Boost", function(state)
    Settings.SpeedBoost.Enabled = state
end)

-- Combat Features
CreateToggle(CombatContent, "Auto Guard", function(state)
    Settings.AutoGuard = state
end)

CreateToggle(CombatContent, "Auto Block", function(state)
    Settings.AutoBlock = state
end)

CreateToggle(CombatContent, "Auto Steal", function(state)
    Settings.AutoSteal = state
end)

CreateToggle(CombatContent, "Sprint Guard", function(state)
    Settings.SprintGuard = state
end)

-- Main Loop
RunService.Heartbeat:Connect(function()
    local char = Player.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end
    
    local hum = char.Humanoid
    local root = char.HumanoidRootPart
    
    -- Speed Boost
    if Settings.SpeedBoost.Enabled then
        local speed = Settings.SpeedBoost.OffBall
        -- Detect if player has ball (simplified)
        if root.Velocity.Magnitude > 30 then
            speed = Settings.SpeedBoost.OnBall
        end
        hum.WalkSpeed = speed
    end
    
    -- Auto Sprint
    if Settings.AutoSprint then
        hum:ChangeState(Enum.HumanoidStateType.Running)
    end
    
    -- Infinite Stamina (simplified)
    if Settings.InfiniteStamina then
        -- Hook stamina values if exist
    end
    
    -- Anti OOB
    if Settings.AntiOOB and root.Position.Y < -50 then
        root.CFrame = CFrame.new(root.Position + Vector3.new(0, 50, 0))
    end
end)

-- Notify
local Notify = Instance.new("TextLabel")
Notify.Parent = MainFrame
Notify.BackgroundTransparency = 1
Notify.Position = UDim2.new(0.5, 0, 0, 5)
Notify.Size = UDim2.new(1, 0, 0, 30)
Notify.Font = Enum.Font.GothamBold
Notify.Text = "⭐ KqzlvoHub Loaded Successfully! ⭐"
Notify.TextColor3 = Color3.fromRGB(100, 255, 100)
Notify.TextScaled = true
Notify.TextStrokeTransparency = 0
Notify.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)

TweenService:Create(Notify, TweenInfo.new(3), {TextTransparency = 1}):Play()
wait(3)
Notify:Destroy()

print("⭐ KqzlvoHub v1.0 Loaded for Practical Basketball ⭐")
